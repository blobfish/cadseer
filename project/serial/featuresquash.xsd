<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  targetNamespace="http://www.cadseer.com/prj/srl"
  xmlns:srl="http://www.cadseer.com/prj/srl"
>
  
<xs:include schemaLocation="featurebase.xsd"/>

<xs:complexType name="FeatureSquash">
  <xs:all>
    <xs:element name="featureBase" type="srl:FeatureBase"/>
    <xs:element name="picks" type="srl:Picks"/>
    <xs:element name="faceId" type="xs:string" default="00000000-0000-0000-0000-000000000000"/>
    <xs:element name="wireId" type="xs:string" default="00000000-0000-0000-0000-000000000000"/>
    <xs:element name="granularity" type="srl:Parameter"/>
    <xs:element name="label" type="srl:PLabel"/>
  </xs:all>
</xs:complexType>

<xs:element name="squash" type="srl:FeatureSquash"/>
  
</xs:schema> 

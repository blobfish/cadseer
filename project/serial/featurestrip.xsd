<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  targetNamespace="http://www.cadseer.com/prj/srl"
  xmlns:srl="http://www.cadseer.com/prj/srl"
>
  
<xs:include schemaLocation="featurebase.xsd"/>

<xs:complexType name="Station">
  <xs:all>
    <xs:element name="text" type="xs:string"/>
    <xs:element name="matrix" type="srl:Matrixd"/>
  </xs:all>
</xs:complexType>

<xs:complexType name="Stations">
  <xs:sequence minOccurs="0" maxOccurs="unbounded">
    <xs:element name="array" type="srl:Station"/>
  </xs:sequence>
</xs:complexType>

<xs:complexType name="FeatureStrip">
  <xs:all>
    <xs:element name="featureBase" type="srl:FeatureBase"/>
    <xs:element name="pitch" type="srl:Parameter"/>
    <xs:element name="width" type="srl:Parameter"/>
    <xs:element name="widthOffset" type="srl:Parameter"/>
    <xs:element name="gap" type="srl:Parameter"/>
    <xs:element name="autoCalc" type="srl:Parameter"/>
    <xs:element name="stripHeight" type="xs:double"/>
    <xs:element name="pitchLabel" type="srl:PLabel"/>
    <xs:element name="widthLabel" type="srl:PLabel"/>
    <xs:element name="widthOffsetLabel" type="srl:PLabel"/>
    <xs:element name="gapLabel" type="srl:PLabel"/>
    <xs:element name="autoCalcLabel" type="srl:PLabel"/>
    <xs:element name="stations" type="srl:Stations"/>
  </xs:all>
</xs:complexType>

<xs:element name="strip" type="srl:FeatureStrip"/>
  
</xs:schema> 

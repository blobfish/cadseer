<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  targetNamespace="http://www.cadseer.com/prj/srl"
  xmlns:srl="http://www.cadseer.com/prj/srl"
>
  
<xs:include schemaLocation="featurebase.xsd"/>

<xs:complexType name="FeatureUnion">
  <xs:all>
    <xs:element name="featureBase" type="srl:FeatureBase"/>
    <xs:element name="intersectionMapper" type="srl:IntersectionMapper"/>
    <xs:element name="targetPicks" type="srl:Picks"/>
    <xs:element name="toolPicks" type="srl:Picks"/>
  </xs:all>
</xs:complexType>

<xs:element name="fUnion" type="srl:FeatureUnion"/>
  
</xs:schema> 
